{
  "type": "module",
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "chokidar": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-session": "latest",
    "multer": "latest",
    "nodemon": "latest",
    "postgres": "latest",
    "react": "next",
    "react-dom": "next",
    "typescript": "rc"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "serve": "node -r dotenv/config ./out/main.js",
    "nodemon": "nodemon -r dotenv/config -w ./out -e js ./out/main.js"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true
    }
  }
}
